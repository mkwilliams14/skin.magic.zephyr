<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="Animation_Home_Slide">
        <animation effect="slide" end="258" time="400" tween="quadratic" condition="Control.HasFocus(301) | Control.HasFocus(302) | [Control.HasFocus(3992) + String.IsEmpty(Window.Property(WidgetFocus))] | [Skin.HasSetting(LockMainMenu) + !$EXP[Exp_InfoVisible]]">Conditional</animation>
        <animation effect="slide" start="0" end="-258" time="400" tween="quadratic" condition="Control.HasFocus(301) | Control.HasFocus(302) | [Control.HasFocus(3992) + String.IsEmpty(Window.Property(WidgetFocus))] | Skin.HasSetting(LockMainMenu)">WindowClose</animation>
        <animation effect="slide" start="-258" end="0" time="400" tween="quadratic" condition="Control.HasFocus(301) | Control.HasFocus(302) | [Control.HasFocus(3992) + String.IsEmpty(Window.Property(WidgetFocus))] | Skin.HasSetting(LockMainMenu)">WindowOpen</animation>
    </include>

    <include name="Animation_KodiLogo">
        <animation effect="fade" start="0" end="100" delay="200" time="500" tween="cubic" easing="inout">Visible</animation>
        <animation effect="fade" start="100" end="0" time="500" tween="cubic" easing="inout">Hidden</animation>
    </include>

    <include name="Animation_Settings">
        <animation type="WindowOpen" reversible="false" condition="!Window.Previous(SettingsCategory.xml) + !Window.Previous(SkinSettings.xml) + !Window.Previous(SettingsProfile.xml) + !Window.Previous(SettingsSystemInfo.xml)">
            <effect type="fade" start="0" end="100" time="400" delay="150"/>
            <effect type="slide" start="0,-48" end="0" center="auto" tween="back" easing="out" time="450" delay="150"/>
        </animation>
    </include>

    <include name="Animation_FocusBounce_Viewlayout">
        <animation type="Focus" reversible="false">
            <effect type="zoom" end="135" time="100" tween="sine" easing="out" center="auto" />
            <effect type="zoom" end="92" time="225" tween="sine" delay="100" easing="out" center="auto" />
        </animation>
    </include>

    <include name="Button_Focus_Animation">
        <animation type="Focus" reversible="false">
            <effect type="zoom" start="100" end="108" time="100" center="auto"/>
            <effect type="zoom" start="100" end="92.592592592593" time="50" delay="100" center="auto"/>
            <!--<effect type="fade" start="10" end="100" time="100"/>-->
        </animation>
    </include>

